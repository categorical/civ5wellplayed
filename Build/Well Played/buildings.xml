<?xml version="1.0" encoding="utf-8"?>

<GameData>
	<Building_YieldChanges>
		<Update>
			<Set>
				<Yield>3</Yield>
			</Set>
			<Where>
				<BuildingType>BUILDING_PAPER_MAKER</BuildingType>
				<YieldType>YIELD_GOLD</YieldType>
			</Where>
		</Update>	
	</Building_YieldChanges>

	<Building_YieldChangesPerPop>
		<Update>
			<Set>
				<Yield>150</Yield>
			</Set>
			<Where>
				<BuildingType>BUILDING_PAPER_MAKER</BuildingType>
				<YieldType>YIELD_SCIENCE</YieldType>
			</Where>
		</Update>
	</Building_YieldChangesPerPop>
	<Buildings>
		<!-- Re-indexes this building, -->
		<!-- so that it has a lower priority when being rendered amongst other wonders. -->
		<!-- This enables it to be re-positioned by other position 4 wonders, -->
		<!-- when the first wonder location of a city looks awkward. -->
		<!-- A city has six positions to display these wonders, -->
		<!-- the six positions seem to be randomly numbered (,but they are static in a game/map). -->
		<!-- Built wonders are picked from lowerest to highest building index to fill in each position, -->
		<!-- i.e. the wonder with lowerest building index occupies the first position.-->
		<Delete>
			<Type>BUILDING_STONEHENGE</Type>
		</Delete>
		<Row>
			<Type>BUILDING_STONEHENGE</Type>
			<BuildingClass>BUILDINGCLASS_STONEHENGE</BuildingClass>
			<Cost>185</Cost>
			<PrereqTech>TECH_CALENDAR</PrereqTech>
			<Description>TXT_KEY_BUILDING_STONEHENGE</Description>
			<Civilopedia>TXT_KEY_WONDER_STONEHENGE_DESC</Civilopedia>
			<Quote>TXT_KEY_WONDER_STONEHENGE_QUOTE</Quote>
			<ArtDefineTag>STONEHENGE</ArtDefineTag>
			<MaxStartEra>ERA_CLASSICAL</MaxStartEra>
			<SpecialistType>SPECIALIST_ENGINEER</SpecialistType>
			<GreatPeopleRateChange>1</GreatPeopleRateChange>
			<NukeImmune>true</NukeImmune>
			<HurryCostModifier>-1</HurryCostModifier>
			<MinAreaSize>-1</MinAreaSize>
			<ConquestProb>100</ConquestProb>
			<DisplayPosition>4</DisplayPosition>
			<IconAtlas>BW_ATLAS_2</IconAtlas>
			<PortraitIndex>2</PortraitIndex>
			<WonderSplashImage>WonderConceptStonehedge.dds</WonderSplashImage>
			<WonderSplashAudio>AS2D_WONDER_SPEECH_STONEHENGE_01</WonderSplashAudio>
		</Row>
	</Buildings>
</GameData>